var express = require("express");
var app = require("../app");
const port = 3001;

// view engine setup
app.set("views", "views");
app.set("view engine", "pug");

app.use(express.json());
app.use(express.urlencoded({ extended: false }));
app.use(express.static("public"));

app.listen(port, () => console.log(`server started on port:${port}`));
